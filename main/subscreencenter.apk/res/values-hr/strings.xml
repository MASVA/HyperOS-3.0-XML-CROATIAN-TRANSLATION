  <string name="ms_guide_tip_camera">Povucite prstom udesno za otvaranje kamere. Povucite prstom ulijevo od ruba za povratak.</string>
  <string name="ms_guide_tip_dial">Pritisnite i držite za uređivanje. Povucite prstom vodoravno za promjenu sata. Dodirnite za potvrdu.</string>
